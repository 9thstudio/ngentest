import { async } from '@angular/core/testing';
import { Injectable } from '@angular/core';

<% for(var key in imports) { -%>
import {<%- imports[key].map(el => `${el}`).join(', ') -%>} from '<%- key -%>';
<% } -%>

describe('<%= className %>', () => {
  let service;

  <% for(var klassName in providerMocks) { %>
  <%- providerMocks[klassName] -%>
  <% } -%>

  beforeEach(() => {
    // TODO: Think about is there anything can do with constructor mocks(?), things done within constructor
    service = new <%= className %>(<%= ctorParamJs %>);
  });

  <% for(var key in functionTests) { -%>
  <%- functionTests[key] -%>
  <% } -%>

});
